{
  "name": "ChampionCounter",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Gets rid of the more button",
  "homepage_url": "http://github.com/vohof/ChampionCounter.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.championcounter.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/inject.js"
      ]
    }
  ]
}
